{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "DigiPlanWebApp": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/DigiPlanWebApp",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "node_modules/ag-grid-community/dist/styles/ag-grid.scss",
              "node_modules/ag-grid-community/dist/styles/ag-theme-balham.css",
              "./src/assets/library/lib/bootstrap/dist/css/bootstrap.min.css",
              "./src/assets/library/lib/bootstrap/dist/css/ErvoStyle.css",
              "node_modules/material-design-icons/iconfont/material-icons.css"
            ],
            "scripts": [
              "./src/assets/library/lib/jquery/dist/jquery.min.js",
              "./src/assets/library/lib/bootstrap/dist/js/bootstrap.bundle.min.js",
              "./src/assets/library/js/site.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb",
                  "maximumError": "200kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "DigiPlanWebApp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "DigiPlanWebApp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "DigiPlanWebApp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [""],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "DigiPlanWebApp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "DigiPlanWebApp:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "DigiPlanWebApp",
  "cli": {
    "analytics": "51a0b966-c9eb-4142-890d-7f295104da2f"
  }
}
